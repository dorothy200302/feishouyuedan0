.container {
  min-height: 100vh;
  background: #f7f8fa;
  padding: 20rpx;
}

.header {
  background: #fff;
  padding: 30rpx;
  border-radius: 12rpx;
  margin-bottom: 20rpx;
}

.title {
  font-size: 32rpx;
  font-weight: bold;
  color: #333;
}

.content {
  background: #fff;
  border-radius: 12rpx;
  padding: 40rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  min-height: 400rpx;
} 